    "url": "https://github.com/lolottetheclash/react-express-starter/issues"
  },
  "homepage": "https://github.com/lolottetheclash/react-express-starter#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
